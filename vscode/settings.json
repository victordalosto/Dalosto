{

    // Excluded files
    "files.exclude": {
        "**/.gitattributes": true,
        "**/.gitignore": true,
        "**/.git": true,
        "**/.github":true,

        "**/.idea": true,
        // "**/Dockerfile": true,

        // "**/.mvn": true,
        // "**/mvnw": true,
        // "**/mvnw.cmd": true,
        // "**/LICENSE": true,
        // "**/HELP.md": true,

        "**/__pytorch__": true,
        "**/__pycache__":true,

        "**/gradlew": true,
        "**/.gradle": true,
        "**/settings.gradle": true,
        "**/gradlew.bat": true,
        "**/gradle": true,
        "**/build": true,
        "**/bin": true,

        "**/node_modules": true,
        "**/package-lock.json": true,
        "**/servidor-api": true,

        "**/obj": true,
        // "**/*.csproj": true,
        // "**/*.csproj.*": true,
        // "**/*.sln": true,


        // "**/build.gradle": true,
        // "**/pom.xml": true,

        "**/.vs": true,
        "**/.vscode": true,
        // "**/Readme.md": true,

        // "**/src/test/**": true,
        "**/target/**": true
    },

    // VSCODE CUSTOMIZATION
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "workbench.colorCustomizations": {

        "editorBracketHighlight.foreground1": "#FC618D", // class
        "editorBracketHighlight.foreground2": "#50FA7B", // blocks-inside-methods
        "editorBracketHighlight.foreground3": "#5ecfe9", // methods
        "editorBracketHighlight.foreground4": "#FFB86C", // additional
        "editorBracketHighlight.foreground5": "#BD93F9", // additional2

        "editor.background": "#0f0f11", // desktop
        "editorGutter.background": "#0f0f11", // numbers

        "sideBar.background": "#000000", // explorer

        "terminal.background": "#000000", // terminal
        "panel.background": "#000000", // terminal header

        "activityBar.background": "#000000", // left-side menu

        "statusBar.background": "#000000", // bottom bar
        "titleBar.activeBackground": "#000000", // top menu

        "tab.activeBackground": "#220736", // tab active
        "tab.activeForeground": "#fffefe",
        "tab.activeBorderTop": "#BD93F9",
        "tab.inactiveBackground": "#353535", // tab inactive
        "tab.inactiveForeground": "#fffefe",

        "tab.border": "#000000",
        "editorGroupHeader.tabsBackground": "#000000", // tab header
        "editorGroupHeader.tabsBorder": "#000000", // tab header border

        "editorGroup.border": "#BD93F9", // tab header border

    },


    "highlight.regexes": {
        "(.*)(using(.*)|namespace(.*)|package(.*)|import(.*))(.*)": [{},{
            "color": "#999996", "fontStyle": ""
        }],
        "(.*)(\\[Key\\]|\\[(.?)+\\])(.*)": [{},{
            "color": "#c0c0c0", "fontStyle": "italic"
        }],
        "(.*)(var | string| int | bool | boolean | char | decimal | double| float | long | short |function )(.*)": [{},{
            "color": "#4de0ff", "fontStyle": ""
        }],
        "(.*)( void )(.*)": [{},{
            "color": "#fc618de1", "fontStyle": "italic"
        }],
        "(.*)( public | private | protected )(.*)": [{},{
            "color": "#FC618D", "fontStyle": ""
        }],
        "(.*)(const |let |var )(.*)": [{},{
            "color": "#7fc2f8", "fontStyle": ""
        }],
        "(.*)( =\\> )(.*)": [{},{
            "color": "#BD93F9", "fontStyle": ""
        }],
        "(.*)( virtual | readonly | static )(.*)": [{},{
            "color": "#FC618Dde", "fontStyle": "italic"
        }],
        "(.*)( static )(.*)": [{},{
            "color": "#FC618Dde", "fontStyle": "italic"
        }],
        "(.*)(@media |@keyframes )(.*)": [{},{
            "color": "#50FA7B", "fontStyle": "italic"
        }],
        "(.*)({(.*)get;(.*)}|{(.*)set;(.*)})(.*)": [{},{
            "color": "#929290", "fontStyle": ""
        }],
        "(.*)( this\\.| self\\.)(.*)": [{},{
        "color": "#FC618D", "fontStyle": "italic"
        }],
        "(.*)(;)": [{},{
            "color": "#565655", "fontStyle": ""
        }],
        "(.*)( null| async|await)(.*)": [{},{
            "color": "#BD93F9", "fontStyle": "italic"
        }],
        "(.*)(\\?| using| foreach|return )(.*)": [{},{
            "color": "#BD93F9", "fontStyle": ""
        }],
        "(.*)(// (.?)+)(.*)": [{},{ // comments
            "color": "#6272A4", "fontStyle": ""
        }],
    },



    // EXTENSIONS
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true
    },




    // CUSTOMIZATION
    "editor.tokenColorCustomizations": {"textMateRules": [

        {   // IMPORTS, PACKAGE,
            "scope": "keyword.control.import.ts, keyword.control.from.ts, storage.modifier.import.java, entity.name.namespace, storage.modifier.package.java, meta.package.java, punctuation.separator.java, keyword.other.import.java, keyword.other.package.java", "settings": {
                "foreground": "#999996", "fontStyle": ""
        }},

        {   // ACESSOR: public, private, proteced
            "scope": "storage.modifier.cs, storage.modifier.java, storage.modifier.ts", "settings": {
                "foreground": "#FC618D", "fontStyle": ""
        }},

        {   // Types
            "scope": "entity.name.type.struct, entity.name.type.cs, keyword.type.cs, entity.name.type.class, entity.name.type.enum, meta.method.identifier.java, entity.name.type.ts, storage.type.primitive.java, support.type.primitive.ts, support.class, entity.name.type.interface, entity.name.namespace", "settings": {
                "foreground": "#4de0ff",
                "fontStyle": "",
        }},

        {   // methods
            "scope": "entity.name.function.extension.cs, entity.name.function, variable.other.object.ts", "settings": {
                "foreground": "#50FA7B", "fontStyle": ""
        }},

        {   // variable method parameter
            "scope": "variable.parameter, entity.name.variable.parameter.cs", "settings": {
                "foreground": "#FFB86C", "fontStyle": ""
        }},

        {   // variable created inside method
            "scope": "entity.name.variable.local.cs, variable.other.readwrite.cs, variable.other.constant, variable.other",
            "settings": {
                "foreground": "#fffffff5", "fontStyle": ""
        }},

        {   // String
            "scope": "string", "settings": {
                "foreground": "#ce9178", "fontStyle": ""
        }},

        {   // Comments
            "scope": "comment, punctuation.definition.comment", "settings": {
                "foreground": "#6272A4", "fontStyle": ""
        }},

        {   // keywords
            "scope": "keyword.control, punctuation.definition.tag.end.html, punctuation.definition.tag.begin.html", "settings": {
                "foreground": "#FC618D", "fontStyle": ""
        }},

        {   // HTML Tags
            "scope": "entity.name.tag.js", "settings": {
                "foreground": "#c8ce7edc", "fontStyle": ""
        }},

        { // HTML DIAMONDS < >
            "scope": "punctuation.definition.tag.begin.js, punctuation.definition.tag.end.js", "settings": {
                "foreground": "#c8ce7edc", "fontStyle": "bold"
            }
        },

        {   // this
            "scope": "keyword, keyword.operator.new, keyword.control.java, variable.language.this.java,variable.language.this.ts", "settings": {
                "foreground": "#FC618D", "fontStyle": ""
        }},

        {   // ponctuator
            "scope": "ponctuation, punctuation.definition, meta.brace, punctuation.section, punctuation.bracket.round", "settings": {
                "foreground": "#FC618D", "fontStyle": ""
        }},

        {   // diamonds <>
            "scope": "punctuation.definition.typeparameters.begin, punctuation.definition.typeparameters.end", "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold",
        }},

        { // .
            "scope": "punctuation.separator.period.java, ponctuation.acessor, punctuation.accessor.ts", "settings": {
                "foreground": "#9f9f99", "fontStyle": ""
            }
        },

        { // && > < <= >=
            "scope": "keyword.operator.logical, keyword.operator.relational", "settings": {
                "foreground": "#BD93F9", "fontStyle": ""
            }
        },

        { // : + = * / -
            "scope": "punctuation.terminator.statement.cs, keyword.operator, punctuation.separator.parameter, entity.name.function.member.overload.cs", "settings": {
                "foreground": "#b3b3ac", "fontStyle": ""
            }
        },

        { // ;
            "scope":"punctuation.terminator.statement.cs, punctuation.terminator.java, punctuation.terminator.statement.ts", "settings": {
                "foreground": "#565655", "fontStyle": ""
            }
        },

        { // quotes as in ' "
            "scope": "punctuation.definition, punctuation.definition.string.begin, punctuation.definition.string.end", "settings": {
                "foreground": "#ce9178", "fontStyle": ""
            }
        },

        {   // STATIC Methods
            "scope": "support, support.function, support.other.variable, variable.other.object.ts", "settings": {
                "foreground": "#BD93F9", "fontStyle": ""
        }},

        {   // Nao definido
            "scope": "entity.name.type", "settings": {
                "foreground": "#fffffff5", "fontStyle": ""
        }},

        {   // Others
            "scope": "constant.numeric", "settings": {
                "fontStyle": ""
        }},

        {   // @Annotation java
            "scope": "storage.type.annotation.java, punctuation.definition.annotation", "settings": {
                "foreground": "#c0c0c0",
                "fontStyle": "italic"
        }},

        {   // FIELD   AND   field in this.FIELD
            "scope": "entity.name.variable.field.cs, variable.other.enummember, entity.name.variable.property.cs, variable.other.constant.property, variable.other.property, variable.other.object.property, support.variable.property", "settings": {
                "foreground": "#ffffff", "fontStyle": "bold"
        }},

        {   // null
            "scope": "constant.language.null.cs", "settings": {
                "foreground": "#BD93F9", "fontStyle": "italic"
        }},
    ]},


    // JAVA CONFIGURATIONS
    //"spring-boot.ls.java.home": "/usr/lib/jvm/java-17-openjdk-amd64/",
    "spring-boot.ls.java.home": "C:\\dev\\Java\\jdk-17",
    "java.jdt.ls.java.home": "C:\\dev\\Java\\jdk-17",
    "maven.terminal.useJavaHome": true,
    "java.maven.downloadSources": true,
    "java.eclipse.downloadSources": true,
    "java.inlayHints.parameterNames.enabled": "all",
    "java.format.settings.url": "https://raw.githubusercontent.com/victordalosto/Dalosto/master/configs/java-formatter.xml",
    "java.debug.settings.vmArgs": "-Xmx8g",

    "java.debug.settings.console": "externalTerminal",
    "terminal.integrated.env.windows": { "PYTHONPATH": "${workspaceFolder}" },
    "terminal.integrated.defaultProfile.linux": "JavaScript Debug Terminal",

    "csharp.suppressBuildAssetsNotification": true,
    "dotnet.server.useOmnisharp": true,
    "csharp.debug.logging.moduleLoad": false,
    "csharp.debug.console": "integratedTerminal",





    // CONFIGS
    "editor.foldingImportsByDefault": true,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "vsintellicode.modify.editor.suggestSelection": "disabled",
    "workbench.editor.enablePreview": false,
    "files.trimTrailingWhitespace": true,



    // VISUAL
    "editor.minimap.enabled": false,
    "workbench.colorTheme": "Dracula",
    "explorer.sortOrder": "filesFirst",
    "workbench.iconTheme": "vscode-icons",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "material-icon-theme.hidesExplorerArrows": true,
    "workbench.tips.enabled": false,
    "window.customMenuBarAltFocus": false,





    // SECURITY
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "security.workspace.trust.untrustedFiles": "open",
    "files.autoSave": "onFocusChange",
    "boot-java.rewrite.reconcile": true,



    // DISABLE ANNOYING BOX
    "editor.parameterHints.enabled": false, // disable pop-up box top
    "editor.inlineSuggest.enabled": true,




    // NEW CONFIGS
    "chatgpt.lang": "en",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "vsicons.presets.hideExplorerArrows": true,
    "vsicons.presets.foldersAllDefaultIcon": true,




    // UI
    "[python]": {"editor.formatOnType": true},
    "[jsonc]": {"editor.defaultFormatter": "vscode.json-language-features"},
    "[xml]": {"editor.defaultFormatter": "redhat.vscode-xml"},


    // TERMINAL TABS
    "terminal.integrated.tabs.enabled": false,

    // OTHER UI
    "redhat.telemetry.enabled": false,
    "window.enableMenuBarMnemonics": false,
    "workbench.layoutControl.enabled": false,
    "csharpextensions.useFileScopedNamespace": true,
    "editor.fontSize": 17,
    "editor.codeLens": false,
    "files.associations": {
        "*.cs": "csharp"
    },
    "window.zoomLevel": 1,
    "editor.fontFamily": "Consolas, Cascadia Mono, monaco monospace, Monaco, 'Courier New'",
    "window.menuBarVisibility": "compact",
    "htmlhint.documentSelector": [
        "js",
        "html",
        "htm"
    ],
    "editor.snippetSuggestions": "top",
    "dotnetAcquisitionExtension.enableTelemetry": false,
    "dotnet.preferRuntimeFromSDK": true,
    "breadcrumbs.enabled": false,
    "workbench.activityBar.visible": false,




}
